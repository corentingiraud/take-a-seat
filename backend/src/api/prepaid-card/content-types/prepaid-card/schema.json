{
  "kind": "collectionType",
  "collectionName": "prepaid_cards",
  "info": {
    "singularName": "prepaid-card",
    "pluralName": "prepaid-cards",
    "displayName": "Carte pré-payée",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "remainingBalance": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "expirationDate": {
      "type": "date",
      "required": false
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "PAID"
      ],
      "required": true,
      "default": "PENDING"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "prepaidCards"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "prepaidCard"
    },
    "validFrom": {
      "type": "date"
    },
    "name": {
      "type": "string"
    },
    "initialBalance": {
      "type": "integer",
      "min": 0
    }
  }
}
